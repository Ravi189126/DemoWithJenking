pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage ('Build'){
            steps {
                echo ' Build Success'
            }
        }
        stage ('Test'){
            steps{
                echo 'Testing success'
            }
        }
        stage ('Deploy Prod'){
            steps{
                echo 'Deployment success'
            }
        }
    }
}
